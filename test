DROP table AD.OrderDetail;
CREATE COLUMN TABLE AD.OrderDetail
(	
	ORDER_DETAIL_IDENTIFIER NVARCHAR(20),
    ORDER_IDENTIFIER NVARCHAR(20),
    PERSON_PHONE_ID NVARCHAR(20),
    PPERSON_PHONE_ID NVARCHAR(20),
    ORDER_DATE DATE,
    AMOUNT	DECIMAL(12,3),
    PRIMARY KEY (ORDER_DETAIL_IDENTIFIER)
);

